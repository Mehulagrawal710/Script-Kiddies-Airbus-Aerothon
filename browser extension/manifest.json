{
	"name" : "Konnex",
	"description" : "Make user experience better",
	"version" : "0.1",
	"manifest_version" : 2,
	"browser_action" : {
		"default_icon" : "icons/konnex_icon.png",
		"default_popup" : "html/popup.html"
	},
	"permissions" : ["activeTab", "tabs", "<all_urls>"],
	"content_scripts": [
		{
			"matches": ["<all_urls>", "http://*/*"],
			"js": ["script/content.js"],
			"css": ["style/styles.css"]
		}
	]
}